<h1>Listing tasks</h1>

<table>
  <tr>
    <th>Sequence</th>
    <th>A</th>
    <th>T</th>
    <th>C</th>
    <th>G</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @tasks.each do |task| %>
  <tr>
    <td><%= link_to task.title,task %></td>
    <td><%= task.a %></td>
    <td><%= task.t %></td>
    <td><%= task.c %></td>
    <td><%= task.g %></td>
    <td><%= link_to 'Show', task %></td>
    <td><%= link_to 'Edit', edit_task_path(task) %></td>
    <td><%= link_to 'Destroy', task, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Task', new_task_path %>
